    <meta name="viewport" content="width=device-width,initial-scale=1" />
    <meta name="tdm-reservation" content="1" />
    <meta
      name="description"
      content="<%= t.meta.description %>"
    />
    <meta property="og:url" content="https://essensmassage.fr<%= lang === 'en' ? '/en/' : '/' %>" />
    <meta property="og:type" content="website" />
    <meta
      property="og:title"
      content="<%= t.meta.ogTitle %>"
    />
    <meta
      property="og:description"
      content="<%= t.meta.ogDescription %>"
    />
    <meta
      property="og:image"
      content="https://opengraph.b-cdn.net/production/images/e5fda67b-2aee-44aa-9e46-fdc1d3273308.png?token=YI3xAFtX_KdSNHs7WVOHMpbXYdDGrHmAPK25t8jD1h4&height=630&width=1200&expires=33293021029"
    />
    <meta name="twitter:card" content="summary_large_image" />
    <meta property="twitter:domain" content="essensmassage.fr" />
    <meta property="twitter:url" content="https://essensmassage.fr<%= lang === 'en' ? '/en/' : '/' %>" />
    <meta
      name="twitter:title"
      content="<%= t.meta.ogTitle %>"
    />
    <meta
      name="twitter:description"
      content="<%= t.meta.ogDescription %>"
    />
    <meta
      name="twitter:image"
      content="https://opengraph.b-cdn.net/production/images/e5fda67b-2aee-44aa-9e46-fdc1d3273308.png?token=YI3xAFtX_KdSNHs7WVOHMpbXYdDGrHmAPK25t8jD1h4&height=630&width=1200&expires=33293021029"
    />
    <% if (lang === 'fr') { %>
      <link rel="canonical" href="https://essensmassage.fr/" />
      <link rel="alternate" href="https://essensmassage.fr/" hreflang="fr" />
      <link rel="alternate" href="https://essensmassage.fr/en/" hreflang="en" />
      <link rel="alternate" href="https://essensmassage.fr/" hreflang="x-default" />
    <% } else { %>
      <link rel="canonical" href="https://essensmassage.fr/en/" />
      <link rel="alternate" href="https://essensmassage.fr/" hreflang="fr" />
      <link rel="alternate" href="https://essensmassage.fr/en/" hreflang="en" />
      <link rel="alternate" href="https://essensmassage.fr/" hreflang="x-default" />
    <% } %>
    <title><%= t.meta.title %></title>
    <link href="<%= assetPath %>/src/css/bootstrap.purged.last.min.css" rel="stylesheet" />
    <link
      rel="icon"
      href="<%= assetPath %>/src/assets/icons/favicon-light.ico"
      type="image/x-icon"
    />
    <link
      rel="icon"
      href="<%= assetPath %>/src/assets/icons/favicon-light.ico"
      type="image/x-icon"
      media="(prefers-color-scheme: light)"
    />
    <link
      rel="icon"
      href="<%= assetPath %>/src/assets/icons/favicon-dark.ico"
      type="image/x-icon"
      media="(prefers-color-scheme: dark)"
    />
  </head>
  <body>
    <%- include('sections/navbar', { t, lang, assetPath }) %>
    <div
      class="overflow-y:scroll;"
      data-bs-spy="scroll"
      data-bs-target=".navbar"
    >
      <%- include('sections/hero', { t, assetPath }) %>
      <%- include('sections/services', { t, assetPath }) %>
      <%- include('sections/about', { t, assetPath }) %>
      <%- include('sections/infos', { t, assetPath }) %>
      <%- include('sections/contact', { t, assetPath }) %>
      <%- include('sections/faq', { t, assetPath }) %>
    </div>
    <%- include('sections/footer', { t, lang, assetPath }) %>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.7/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-ndDqU0Gzau9qJ1lfW4pNLlhNTkCfHzAVBReH9diLvGRem5+R9g2FzA8ZGN954O5Q"
      crossorigin="anonymous"
      defer
    >
      ;
    </script>
    <script src="<%= assetPath %>/src/js/custom.min.js" defer>
      ;
    </script>
  </body>
</html>
