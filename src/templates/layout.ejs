<!doctype html>
<html lang="<%= lang %>">
  <head>
    <meta charset="UTF-8" />
    <script src="<%= assetPath %>/src/tarteaucitron/tarteaucitron.min.js">
      ;
    </script>
    <script type="text/javascript">
      tarteaucitron.init({
        privacyUrl: "",
        bodyPosition: "top",
        hashtag: "#tarteaucitron",
        cookieName: "tarteaucitron",
        orientation: "bottom",
        groupServices: !1,
        showDetailsOnClick: !0,
        serviceDefaultState: "wait",
        showAlertSmall: !1,
        cookieslist: !1,
        cookieslistEmbed: !1,
        closePopup: !0,
        showIcon: !0,
        iconSrc: "<%= assetPath %>/src/assets/icons/c-circle.svg",
        iconPosition: "BottomRight",
        adblocker: !1,
        DenyAllCta: !0,
        AcceptAllCta: !0,
        highPrivacy: !0,
        alwaysNeedConsent: !1,
        handleBrowserDNTRequest: !1,
        removeCredit: !1,
        moreInfoLink: !0,
        useExternalCss: !1,
        useExternalJs: !1,
        readmoreLink: "",
        mandatory: !1,
        mandatoryCta: !1,
        googleConsentMode: !0,
        bingConsentMode: !1,
        softConsentMode: !1,
        dataLayer: !1,
        serverSide: !1,
        partnersList: !0,
      });
    </script>
    <script>
      ((tarteaucitron.user.gtagUa = "G-NEST0W5PSV"),
        (tarteaucitron.user.gtagMore = function () {}),
        (tarteaucitron.job = tarteaucitron.job || []).push("gtag"));
    </script>
    <script type="application/ld+json">
      {
        "@context": "https://schema.org",
        "@graph": [
          {
            "@type": "HealthAndBeautyBusiness",
            "@id": "https://essensmassage.fr<%= lang === 'en' ? '/en/' : '/' %>#business",
            "name": "EssensMassage",
            "image": "https://essensmassage.fr/logo50x50.png",
            "url": "https://essensmassage.fr<%= lang === 'en' ? '/en/' : '/' %>",
            "logo": "https://essensmassage.fr/logo50x50.png",
            "description": "<%= t.schema.description %>",
            "slogan": "<%= t.schema.slogan %>",
            "telephone": "+33 6 52 32 08 01",
            "email": "rdv@essensmassage.fr",
            "priceRange": "€€",
            "founder": { "@type": "Person", "name": "Pascal Piallat" },
            "address": [
              {
                "@type": "PostalAddress",
                "addressLocality": "Lasalle",
                "addressRegion": "Occitanie",
                "postalCode": "30460",
                "addressCountry": "FR"
              },
              {
                "@type": "PostalAddress",
                "addressLocality": "Courchevel",
                "addressRegion": "Auvergne-Rhône-Alpes",
                "postalCode": "73120",
                "addressCountry": "FR"
              }
            ],
            "areaServed": [
              { "@type": "Place", "name": "Courchevel" },
              { "@type": "Place", "name": "Lasalle" }
            ],
            "openingHoursSpecification": [
              {
                "@type": "OpeningHoursSpecification",
                "dayOfWeek": [
                  "Monday",
                  "Tuesday",
                  "Wednesday",
                  "Thursday",
                  "Friday",
                  "Saturday",
                  "Sunday"
                ],
                "opens": "10:00",
                "closes": "20:00"
              }
            ],
            "sameAs": [
              "https://www.google.com/maps/place/EssensMassage",
              "https://www.instagram.com/essensmassage",
              "https://www.facebook.com/essensmassage"
            ],
            "hasOfferCatalog": {
              "@type": "OfferCatalog",
              "name": "<%= t.schema.catalogName %>",
              "itemListElement": [
                {
                  "@type": "Offer",
                  "itemOffered": {
                    "@type": "Service",
                    "name": "<%= t.schema.serviceNames.tuina %>",
                    "description": "<%= t.schema.serviceDescriptions.tuina %>"
                  }
                },
                {
                  "@type": "Offer",
                  "itemOffered": {
                    "@type": "Service",
                    "name": "<%= t.schema.serviceNames.sports %>",
                    "description": "<%= t.schema.serviceDescriptions.sports %>"
                  }
                },
                {
                  "@type": "Offer",
                  "itemOffered": {
                    "@type": "Service",
                    "name": "<%= t.schema.serviceNames.relaxing %>",
                    "description": "<%= t.schema.serviceDescriptions.relaxing %>"
                  }
                }
              ]
            },
            "inLanguage": "<%= t.schema.inLanguage %>"
          }
        ]
      }
